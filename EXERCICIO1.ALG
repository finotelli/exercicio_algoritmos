Algoritmo "exercicio"
Var
   x, y, z: inteiro
   
Inicio
      // entrada
      escreval("Digite um valor para x: ")
      leia(x)
      escreval("digite um valor para y: ")
      leia(y)
      
      //processamento
      z <- (x*y) + 5
      x <- y
      
      // saida
      escreval("x=", x, " ", "y=", y, " ", "z=", z)
Fimalgoritmo